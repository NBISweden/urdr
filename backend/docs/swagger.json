{
    "swagger": "2.0",
    "info": {
        "description": "This is the Urdr API.",
        "title": "Urdr API",
        "contact": {
            "name": "National Bioinformatics Infrastructure Sweden",
            "url": "https://www.nbis.se"
        },
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/api/login": {
            "post": {
                "security": [
                    {
                        "BasicAuth": []
                    }
                ],
                "description": "Log in a user using the Redmine API",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Log in a user",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/logout": {
            "post": {
                "description": "Log out a user by destroying the session",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Log out a user",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/report": {
            "post": {
                "description": "report time spent on issues",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "report time spent on issues",
                "parameters": [
                    {
                        "description": "urdr_session=default",
                        "name": "time_entry",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/redmine.TimeEntry"
                        }
                    },
                    {
                        "type": "string",
                        "description": "default",
                        "name": "Cookie",
                        "in": "header",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "redmine.TimeEntry": {
            "type": "object",
            "properties": {
                "activity_id": {
                    "type": "integer"
                },
                "comments": {
                    "type": "string"
                },
                "hours": {
                    "type": "integer"
                },
                "issue_id": {
                    "type": "integer"
                },
                "spent_on": {
                    "type": "string"
                },
                "user_id": {
                    "type": "integer"
                }
            }
        }
    },
    "securityDefinitions": {
        "BasicAuth": {
            "type": "basic"
        }
    }
}